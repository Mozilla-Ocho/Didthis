// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  # i am getting segfaults on GCP cloud run. not sure why, this issue suggests
  # switching from the library (default) to the binary prisma engine
  # https://github.com/prisma/prisma/issues/10649
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DummyRecord {
  id    Int     @id @default(autoincrement())
  name  String  @unique
}

